#include <iostream>
using namespace std;
 
int main()
{
 int arr = 10;
 int ar[arr] = { 12, 5, 3, 2, 45, 96, 6, 8, 11, 24 };
 
 int buff = 0; // для хранения перемещаемого значения
 int i, j; // для циклов 
 for (i = 1; i < arr; i++) {
 buff = ar[i];
 for (j = i - 1; j >= 0 && ar[j] > buff; j--)
 ar[j + 1] = ar[j]; 
 
 ar[j + 1] = buff;
 }
 for (int i = 0; i < arr; i++) // вывод отсортированного массива
 cout << ar[i] << " " ;
}
